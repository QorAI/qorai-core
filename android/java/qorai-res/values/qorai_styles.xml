        <item name="hintTextColor">@color/text_primary</item>
        <!-- Blinking cursor color. -->
        <item name="cursorColor">@color/icon_interactive</item>
        <!-- Blinking cursor color on error state. -->
        <item name="cursorErrorColor">@color/system_feedback_error</item>
    </style>

