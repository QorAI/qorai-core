        <!-- as we don't want to change it for the whole app. -->
        <item name="colorControlActivated">@color/icon_interactive</item>
        <!-- Hint text color that translates to the top of the view. -->
        <item name="android:textColorHint">@color/text_primary</item>
    </style>

