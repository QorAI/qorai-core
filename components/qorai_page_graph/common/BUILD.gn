import("//qorai/components/qorai_page_graph/common/buildflags.gni")
import("//build/buildflag_header.gni")

buildflag_header("buildflags") {
  header = "buildflags.h"
  flags = [
    "ENABLE_QORAI_PAGE_GRAPH=$enable_qorai_page_graph",
    "ENABLE_QORAI_PAGE_GRAPH_WEBAPI_PROBES=$enable_qorai_page_graph_webapi_probes",
  ]
}

source_set("common") {
  sources = [
    "features.cc",
    "features.h",
  ]

  deps = [ "//base" ]
}
