import("//qorai/components/ipfs/buildflags/buildflags.gni")
import("//testing/test.gni")
source_set("qorai_ipfs_unit_tests") {
  testonly = true

  sources = [ "//qorai/components/ipfs/ipfs_utils_unittest.cc" ]

  if (deprecate_ipfs) {
    sources += [ "//qorai/components/ipfs/ipfs_component_cleaner_unittest.cc" ]
  }

  deps = [
    "//base/test:test_support",
    "//qorai/components/ipfs",
    "//chrome/browser",
    "//chrome/browser/prefs",
    "//chrome/browser/sync",
    "//chrome/browser/sync:factories",
    "//chrome/test:test_support",
    "//components/sync_preferences:test_support",
    "//content/test:test_support",
    "//net",
  ]
}  # source_set("qorai_ipfs_unit_tests")
