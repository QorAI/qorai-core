import("//build/buildflag_header.gni")

declare_args() {
  qorai_stats_api_key = ""
}

buildflag_header("buildflags") {
  header = "buildflags.h"
  flags = [ "QORAI_STATS_API_KEY=\"$qorai_stats_api_key\"" ]
}

static_library("browser") {
  sources = [
    "qorai_stats_updater_util.cc",
    "qorai_stats_updater_util.h",
  ]

  deps = [
    ":buildflags",
    "//base",
    "//qorai/components/constants",
    "//components/prefs",
  ]
}
