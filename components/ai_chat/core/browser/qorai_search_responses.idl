// Schema for Qorai Search responses.
namespace qorai_search_responses {
  dictionary Answer {
    DOMString text;
  };

  dictionary QuerySummary {
    Answer[] answer;
    DOMString query;
  };

  dictionary QuerySummaryResponse {
    QuerySummary[] conversation;
  };
};
